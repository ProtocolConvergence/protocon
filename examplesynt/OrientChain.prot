    ( cgt[2*i]==1 && cgt[2*i+1]==1 && flipped[i-1]==0 && flipped[i+1]==1 && way[2*i-1]==1 && way[2*i+2]==0 && way[2*i]==0 && way[2*i+1]==1 && flipped[i]==0 --> way[2*i]:=0; way[2*i+1]:=1; flipped[i]:=1; )
    ( cgt[2*i]==1 && cgt[2*i+1]==1 && flipped[i-1]==1 && flipped[i+1]==0 && way[2*i-1]==0 && way[2*i+2]==1 && way[2*i]==1 && way[2*i+1]==0 && flipped[i]==0 --> way[2*i]:=1; way[2*i+1]:=0; flipped[i]:=1; )
    ( cgt[2*i]==0 && cgt[2*i+1]==1 && flipped[i-1]==0 && flipped[i+1]==0 && way[2*i-1]==0 && way[2*i+2]==1 && way[2*i]==0 && way[2*i+1]==1 && flipped[i]==1 --> way[2*i]:=1; way[2*i+1]:=0; flipped[i]:=0; )
    ( cgt[2*i]==1 && cgt[2*i+1]==0 && flipped[i-1]==0 && flipped[i+1]==0 && way[2*i-1]==1 && way[2*i+2]==0 && way[2*i]==1 && way[2*i+1]==0 && flipped[i]==1 --> way[2*i]:=0; way[2*i+1]:=1; flipped[i]:=0; )
