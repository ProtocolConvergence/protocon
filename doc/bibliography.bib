@TECHREPORT{tacas04,
 AUTHOR = {Sandeep S. Kulkarni and Ali Ebnenasir},
 TITLE = {Adding Fault-Tolerance Using Pre-Synthesized Components},
 NUMBER = {MSU-CSE-03-28},
 INSTITUTION = {Computer Science and Engineering, Michigan State University},
 ADDRESS = {East Lansing, Michigan},
 ABSTRACT = {We present a synthesis method where we automatically add pre-synthesized fault-tolerance components to fault-intolerant programs. Our synthesis method has the advantage of reusing pre-synthesized fault-tolerance components. Moreover, using pre-synthesized components prevents the synthesis algorithm from declaring failure where the existing heuristic-based approaches fail. We present a sound algorithm for adding distributed fault-tolerance components to distributed programs. As an illustration, we synthesize a token ring program that tolerates the corruption of all processes using a pre-synthesized component. Also, we have reused the same component in the synthesis of a fault-tolerant alternating bit protocol.},
 KEYWORDS = {Automatic addition of fault-tolerance, Distributed programs, Formal methods, Program synthesis, Detectors, Correctors},
 NOTE = {},
